all:
	$(MAKE) -C solver/
	$(MAKE) -C generator/

re:
	$(MAKE) -C solver/ re
	$(MAKE) -C generator/ re

clean:
	$(MAKE) -C solver/ clean
	$(MAKE) -C generator/ clean

fclean:
	$(MAKE) -C solver/ fclean
	$(MAKE) -C generator/ fclean
